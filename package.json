{
  "name": "servie-cookie-store",
  "version": "1.1.0",
  "description": "Cookie storage and manipulation for Servie",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "build": "rimraf dist && tsc",
    "specs": "jest --coverage",
    "test": "npm run -s lint && npm run -s build && npm run -s specs",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/serviejs/servie-cookie-store.git"
  },
  "keywords": [
    "servie",
    "cookie",
    "http",
    "session",
    "store"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/serviejs/servie-cookie-store/issues"
  },
  "homepage": "https://github.com/serviejs/servie-cookie-store",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "ts-jest/preprocessor.js"
    },
    "testRegex": "src/.*\\.(?:test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "jest": "^22.4.4",
    "keysign": "^1.0.1",
    "rimraf": "^2.5.4",
    "servie": "^3.0.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.3",
    "typings": "^2.1.0",
    "universal-base64": "^1.0.0"
  },
  "peerDependencies": {
    "servie": "*"
  },
  "dependencies": {
    "@types/cookie": "^0.3.0",
    "cookie": "^0.3.1"
  }
}
